{
  "expo": {
    "name": "Markaz App",
    "slug": "markaz-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/markaz-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.markazapp",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
            "fetch"
        ]
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/markaz-icon.png",
      "publicPath": "/app"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "expo-audio",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/noti-icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/audio/azaan.wav"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.anonymous.markazapp",
      "permissions": [
        "POST_NOTIFICATIONS",
        "SCHEDULE_EXACT_ALARM"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/markaz-icon.png",
        "backgroundColor": "#059669"
      }
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "49718800-5134-4baf-8242-2707af98fdc1"
      }
    },
    "owner": "mubashiransari"
  }
}
